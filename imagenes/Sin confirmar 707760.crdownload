		<script type="text/javascript">
			$(window).on('load', function() {
				$('.status').fadeOut();
				$('#preloader').fadeOut('slow');
				$('body').css({
					'overflow': 'visible'
				});
				$( ".countdown__character" ).addClass( "animate" );
			})

			$('.modes__slide').slick({
			  infinite: true,
			  slidesToShow: 3,
			 	arrow: false,
				responsive: [
					{
						breakpoint: 1024,
						settings: {
							slidesToShow: 2,
							arrow: true
						}
					},
					{
						breakpoint: 640,
						settings: {
							slidesToShow: 1,
							arrow: true
						}
					}
				]
			});


			$('.discover__slide').slick({
				infinite: true,
				slidesToShow: 1,
				slidesToScroll: 1,
				fade: true,
				autoplay: true,
				speed: 500,
				responsive: [
			    {
			      breakpoint: 1024,
			      settings: {
			        fade: false,
			      }
			    }
			  ]
			});

			AOS.init();

			// headroom

			// grab an element
			var myElement = document.querySelector(".nav-container");
			// construct an instance of Headroom, passing the element
			var headroom = new Headroom(myElement, {
			  "offset": 0,
			  "tolerance": 5,
			  "classes": {
			    "initial": "headroom",
			    "pinned": "headroom--pinned",
			    "unpinned": "headroom--unpinned"
			  }
			});
			// initialise
			headroom.init();
		</script>
